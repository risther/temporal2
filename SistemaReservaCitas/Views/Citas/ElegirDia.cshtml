@model List<SistemaReservaCitas.Models.HorarioAtencionDia>
@{
    ViewBag.Title = "Index";
}






<br />
<table id="DatatableP" class="table">
    <thead>
        <tr>
            <th>ID Atencion Dia </th>
            <th>DIA</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@(m.horarioAtencionDia_id)</td>
                <td>@(m.dia)</td>
                <td style="align-content:center">

                    <a href="~/HorarioDiaDetalle/ElegirHoraCita/@m.horarioAtencionDia_id" class="btn btn-primary">Elegir este Dia</a>
                </td>
            </tr>
        }
    </tbody>
</table>
